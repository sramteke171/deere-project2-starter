    <!DOCTYPE html>
    <html lang="en">
      <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Rooms for Design</title>
        <link rel="stylesheet" href="/css/roomIndex.css" />
      </head>
      <body>
        <h2>Rooms for Design</h2>
    
        <div class="wrapperDiv">
          <header class="grid-item">
            <nav>
              <ul>
                <li>
                  <h4><a href="/roomsC/new">  New Room  </a> </h4>
                </li>
                <li>
                  <h4><a href="/"> Homepage </a></h4>
                </li>
              </ul>
            </nav>
          </header>
        </div>


    <section>
    <div>
      <ul class="room">
          <% for(let i=0; i<rooms.length; i++) { %>
              <li>
                  <h4> Design <a href="/roomsC/<%=rooms[i].id%>/design"> <label> <%=rooms[i].name%> </label</h4> 
                  <img src="<%= rooms[i].img%>" alt="" /> </a>
                  
                <form action="/roomsC/<%=rooms[i].id %>?_method=DELETE" method="POST">
                  <input type="submit" value="DELETE"/>
                </form>
                <a href="/roomsC/<%=rooms[i].id %>/edit"> <h4> <label> Edit </label> </h4></a>
              </li>
          <% } %>
      </ul>
      </div>
      </section> 

      
